{
    "name": "kodeine/laravel-acl",
    "description": "Light-weight role-based permissions for Laravel 5 built in Auth system.",
    "license": "MIT",
    "keywords": [
        "laravel",
        "permissions",
        "roles",
        "auth",
        "acl",
        "security",
        "eloquent"
    ],
    "authors": [
        {
            "name": "Ahsen M.",
            "homepage": "https://github.com/kodeine",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^12.0|^11.0|^10.0|^9.0|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "orchestra/testbench": "*",
        "doctrine/dbal": "*"
    },
    "autoload": {
        "psr-4": {
            "Kodeine\\Acl\\": "src/Kodeine/Acl"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kodeine\\Acl\\Tests\\": "tests/"
        }
    },
    "extra": {
	    "laravel": {
		    "providers": [
                "Kodeine\\Acl\\AclServiceProvider"
            ]
	    }
    }
}
